{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"env": {
				"DYLD_LIBRARY_PATH": "/Users/zbhe/homebrew/lib:${env:DYLD_LIBRARY_PATH}"
			},
			"label": "rust: cargo run"
		},
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo build"
		}
	]
}